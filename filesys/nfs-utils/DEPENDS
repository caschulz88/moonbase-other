depends portmap
depends quota
depends libcap
depends libevent
depends libnfsidmap
depends lvm2
depends libtirpc

optional_depends sqlite       "--enable-nfsdcltrack" "--disable-nfsdcltrack" "build NFSv4 clientid tracking programs"
optional_depends librpcsecgss "--enable-gss"         "--disable-gss"         "support rpcsec_gss (Kerberos)"
